{
  "name": "claude-symphony",
  "version": "0.2.5",
  "description": "Multi-AI Orchestration Framework - Create new projects with 10-stage development workflow",
  "type": "module",
  "bin": {
    "claude-symphony": "./dist/cli/index.js"
  },
  "files": [
    "dist/",
    "template/",
    "assets/",
    "schemas/",
    "scripts/"
  ],
  "scripts": {
    "build": "tsup && npm run build:schema",
    "build:schema": "tsx scripts/build-schema.ts",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src --ext .ts",
    "clean": "rm -rf dist",
    "migrate:jsonc": "tsx scripts/migrate-yaml-to-jsonc.ts",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "ai",
    "workflow",
    "pipeline",
    "claude",
    "gemini",
    "codex",
    "multi-ai",
    "orchestration",
    "symphony"
  ],
  "author": "znehraks@gmail.com",
  "license": "MIT",
  "engines": {
    "node": ">=20.12.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/znehraks/claude-symphony"
  },
  "bugs": {
    "url": "https://github.com/znehraks/claude-symphony/issues"
  },
  "homepage": "https://github.com/znehraks/claude-symphony#readme",
  "dependencies": {
    "@inquirer/prompts": "^7.10.1",
    "chalk": "^5.4.1",
    "commander": "^13.1.0",
    "execa": "^9.5.2",
    "js-yaml": "^4.1.0",
    "jsonc-parser": "^3.3.1",
    "ora": "^8.1.1",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^22.10.7",
    "tsup": "^8.3.5",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3",
    "vitest": "^3.0.4",
    "zod-to-json-schema": "^3.24.1"
  }
}
